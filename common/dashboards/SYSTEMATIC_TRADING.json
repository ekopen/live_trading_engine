          "pluginVersion": "4.11.2",
          "queryType": "timeseries",
          "rawSql": "SELECT\r\n    time_bucket AS time,\r\n    category AS metric,\r\n    SUM(avg_value) * 5 as portfolio_value\r\nFROM\r\n(\r\n    SELECT\r\n        toStartOfInterval(time, INTERVAL 30 MINUTE) AS time_bucket,\r\n        CASE\r\n            WHEN startsWith(strategy_name, 'Long') THEN 'Long Only'\r\n            ELSE 'ML Strategies'\r\n        END AS category,\r\n        AVG(portfolio_value) AS avg_value\r\n    FROM portfolio_db_ts\r\n    WHERE $__timeFilter(time)\r\n    GROUP BY\r\n        time_bucket,\r\n        category\r\n)\r\nGROUP BY\r\n    time_bucket,\r\n    category\r\nORDER BY\r\n    time_bucket;\r\n",
          "refId": "A"
        }
      ],
